{
  "extends": ["next", "next/core-web-vitals", "prettier"],
  "rules": {
    "comma-dangle": ["error", "always-multiline"],
    "eol-last": "error",
    "import/order": [
      "error",
      { "groups": ["builtin", "external", "parent", "sibling", "index"] }
    ],
    "no-console": "warn",
    "quotes": ["error", "single"],
    "semi": ["error", "always"]
  }
}
